{
  "installCommand": "corepack enable && cp ../../pnpm-lock.yaml ./pnpm-lock.yaml && pnpm install --frozen-lockfile",
  "buildCommand": "pnpm -C ../../ build --filter=showcase",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/cosmic",
      "destination": "/?variant=cosmic"
    }
  ]
}