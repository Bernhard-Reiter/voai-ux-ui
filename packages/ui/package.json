{
  "name": "@voai/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm build:tokens && tsup",
    "build:tokens": "style-dictionary build --config ./tokens/config.json",
    "dev": "pnpm build:tokens && tsup --watch",
