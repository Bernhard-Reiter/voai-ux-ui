{
  "name": "@voai/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": ["**/*.css"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "src/styles/**/*.css",
    "tailwind.preset.js"
  ],
  "scripts": {
    "build": "pnpm build:tokens && tsup",
    "build:tokens": "style-dictionary build --config ./tokens/config.json",
    "dev": "pnpm build:tokens && tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "postinstall": "pnpm build:tokens"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.0.17",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.0.17",
    "@storybook/react-vite": "^9.0.17",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@voai/eslint-config": "workspace:*",
    "@voai/tsconfig": "workspace:*",
    "chromatic": "^13.1.2",
    "eslint": "^8.57.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "react": "^18.2.0",
    "storybook": "^9.0.17",
    "ts-jest": "^29.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.6",
    "react-hook-form": "^7.60.0",
    "style-dictionary": "^3.9.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "zod": "^4.0.5"
  }
}