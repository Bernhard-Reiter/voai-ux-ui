{
  "name": "voai-monorepo",
  "version": "0.0.0",
  "description": "voai CRM & Frontend Monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "commit": "cz",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "keywords": [
    "monorepo",
    "voai",
    "crm",
    "frontend"
  ],
  "author": "voai Team",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.2",
    "turbo": "^2.0.6"
  },
  "packageManager": "pnpm@8.15.9",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  }
}
